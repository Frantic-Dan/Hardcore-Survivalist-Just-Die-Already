<configs>
	<!--increased zombie spawning-->
	<set xpath="/spawning/biome[1]//spawn[1]/@maxcount">2</set>
	<set xpath="/spawning/biome[1]//spawn[2]/@maxcount">2</set>
	<set xpath="/spawning/biome[1]//spawn[1]/@respawndelay">1</set>
	<set xpath="/spawning/biome[1]//spawn[2]/@respawndelay">1.1</set>
	
	<set xpath="/spawning/biome[2]//spawn[1]/@maxcount">2</set>
	<set xpath="/spawning/biome[2]//spawn[2]/@maxcount">2</set>
	<set xpath="/spawning/biome[2]//spawn[1]/@respawndelay">0.9</set>
	<set xpath="/spawning/biome[2]//spawn[2]/@respawndelay">1.1</set>	
	
	<set xpath="/spawning/biome[3]//spawn[1]/@maxcount">2</set>
	<set xpath="/spawning/biome[3]//spawn[2]/@maxcount">2</set>
	<set xpath="/spawning/biome[3]//spawn[1]/@respawndelay">0.9</set>
	<set xpath="/spawning/biome[3]//spawn[2]/@respawndelay">0.7</set>
	
	<remove xpath="/spawning//biome[@name='snow']"/>
	<insertafter xpath="/spawning//biome[@name='desert']">
	<biome name="snow">
		<spawn maxcount="2" respawndelay="0.9" time="Any" entitygroup="SnowBiomeDIEmod" />
		<spawn maxcount="2" respawndelay="0.6" time="Night" entitygroup="SnowBiomeDIEmod" />
		<spawn maxcount="1" respawndelay="1" time="Any" entitygroup="WildGameForest" spawnDeadChance="0" />
		<spawn maxcount="1" respawndelay="3.1" time="Any" entitygroup="EnemyAnimalsSnow" spawnDeadChance="0" />
	</biome>
	</insertafter>
	
	<set xpath="/spawning/biome[5]//spawn[1]/@maxcount">2</set>
	<set xpath="/spawning/biome[5]//spawn[2]/@maxcount">5</set>
	<set xpath="/spawning/biome[5]//spawn[1]/@respawndelay">0.2</set>
	<set xpath="/spawning/biome[5]//spawn[2]/@respawndelay">0</set>
	
	
	
	
</configs>