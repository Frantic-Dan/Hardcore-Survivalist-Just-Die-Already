<configs>
	
	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='food']/display_entry[@name='.foodStaminaBonusAdd']">
		<display_entry name="dDysenteryRisk" title_key="statDysenteryRisk" display_type="Percent" negative_preferred="true"/>
	</insertAfter>

	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='foodWaterRaw']/display_entry[@name='ddummyNoBonus']">
		<display_entry name="dDysenteryRisk" title_key="statDysenteryRisk" display_type="Percent" negative_preferred="true"/>
	</insertAfter>
	
	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='medicalFirstAidKit']//display_entry[@name='dStopsLaceration']">
		<display_entry name="dStopsConcussion" title_key="statStopsConcussion" display_type="Percent"/>
	</insertAfter>
	
	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='medical']//display_entry[@name='dStopsBleeding']">
		<display_entry name="dStopsLaceration" title_key="statStopsLaceration" display_type="Percent"/>
			<display_entry name="dDiseaseResist" title_key="statDiseaseResist" display_type="Percent"/>
			<display_entry name="dStopsWeakness" title_key="statStopsWeakness" display_type="Percent"/>
			<display_entry name="dStopsConcussion" title_key="statStopsConcussion" display_type="Percent"/>
			<display_entry name="dCureInfection" title_key="statCureInfection" display_type="Percent"/>
	</insertAfter>
	
	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='medicalFirstAidKit']">
	<item_display_info display_type="medicalTraumaKit" display_group="groupConsumables">
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="dhealAbrasionMult" title_key="statHealAbrasionMult" display_type="Percent"/>
			<display_entry name="dStopsLaceration" title_key="statStopsLaceration" display_type="Percent"/>
			<display_entry name="dDiseaseResist" title_key="statDiseaseResist" display_type="Percent"/>
			<display_entry name="dStopsWeakness" title_key="statStopsWeakness" display_type="Percent"/>
			<display_entry name="dStopsConcussion" title_key="statStopsConcussion" display_type="Percent"/>
			<display_entry name="dCureInfection" title_key="statCureInfection" display_type="Percent"/>
		</item_display_info>
	</insertAfter>

	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='foodShamChowder']">
	<item_display_info display_type="foodGumboStew" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name=".foodStaminaBonusAdd" title_key="statFoodStaminaBonus"/>
			<display_entry name="dIntellect" title_key="statShowIntellect" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>	
	
	
	<item_display_info display_type="foodShepardsPie" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name=".foodStaminaBonusAdd" title_key="statFoodStaminaBonus"/>
			<display_entry name="dStrength" title_key="statShowStrength" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>	
	
	
	<item_display_info display_type="foodSpaghetti" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name=".foodStaminaBonusAdd" title_key="statFoodStaminaBonus"/>
			<display_entry name="dAgility" title_key="statShowAgility" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>	
	
	
	<item_display_info display_type="foodTunaFishGravyToast" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name=".foodStaminaBonusAdd" title_key="statFoodStaminaBonus"/>
			<display_entry name="dPerception" title_key="statShowPerception" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>	
	</insertAfter>	
	
	<remove xpath="/ui_display_info/item_display/item_display_info[@display_type='drinkYuccaSmoothie']"/>
	
	<insertAfter xpath="/ui_display_info/item_display/item_display_info[@display_type='foodPumpkinCheesecake']">
	<item_display_info display_type="drinkYuccaSmoothie" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dHyperthermalResist" title_key="statHyperthermalResist" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>
	
	</insertAfter>
	
</configs>