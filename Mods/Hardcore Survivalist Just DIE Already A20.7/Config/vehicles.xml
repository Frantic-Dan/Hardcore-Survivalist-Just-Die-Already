<configs>
	<!--makes Gyrocopter a two-seater-->
	<insertAfter xpath="/vehicles/vehicle[@name='vehicleGyrocopter']//property[@class='seat0']">
	<property class="seat1">
		<property name="position" value="0, .05, .78"/>
		<property name="rotation" value="-25, 0, 0"/>
		<property name="exit" value="-1.3,0,.9 ~ 1.3,0,.9 ~ 0,0,-2.4 ~ 0,0,2.4 ~ 0,2,0"/>
	</property>
	</insertAfter>
</configs>